<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/toHomeFragment">

    <navigation
        android:id="@+id/toHomeFragment"
        app:startDestination="@id/homeFragment">

        <fragment
            android:id="@+id/homeFragment"
            android:name="com.example.scooby.scooby.home.HomeFragment"
            android:label="fragment_home"
            tools:layout="@layout/fragment_home">
            <action
                android:id="@+id/action_homeFragment_to_blogsFragment"
                app:destination="@id/blogsFragment" />
            <action
                android:id="@+id/action_homeFragment_to_servicesFragment"
                app:destination="@id/servicesFragment" />
            <action
                android:id="@+id/action_homeFragment_to_vetFragment"
                app:destination="@id/vetFragment" />
            <action
                android:id="@+id/action_homeFragment_to_menuBottomSheetFragment"
                app:destination="@id/menuBottomSheetFragment" />
            <action
                android:id="@+id/action_homeFragment_to_selectPetFragment"
                app:destination="@id/selectPetFragment" />
            <action
                android:id="@+id/action_homeFragment_to_profileFragment"
                app:destination="@id/profileFragment" />
        </fragment>
        <fragment
            android:id="@+id/blogsFragment"
            android:name="com.example.scooby.scooby.ui.BlogsFragment"
            android:label="fragment_blogs"
            tools:layout="@layout/fragment_blogs" />
        <fragment
            android:id="@+id/servicesFragment"
            android:name="com.example.scooby.scooby.services.fragment.ServiceFragment"
            android:label="fragment_services"
            tools:layout="@layout/fragment_service">
            <action
                android:id="@+id/action_servicesFragment_to_servicesProfileFragment"
                app:destination="@id/servicesProfileFragment" />
        </fragment>
        <fragment
            android:id="@+id/servicesProfileFragment"
            android:name="com.example.scooby.scooby.services.fragment.ServicesProfileFragment"
            android:label="ServicesProfileFragment"
            tools:layout="@layout/fragment_services_profile">
            <argument
                android:name="ServiceId"
                app:argType="string" />
        </fragment>
        <fragment
            android:id="@+id/doctorsFragment"
            android:name="com.example.scooby.scooby.doctor.DoctorsFragment"
            android:label="fragment_doctors"
            tools:layout="@layout/fragment_doctors" >
            <action
                android:id="@+id/action_doctorsFragment_to_doctorProfileFragment"
                app:destination="@id/doctorProfileFragment" />
        </fragment>
        <fragment
            android:id="@+id/doctorProfileFragment"
            android:name="com.example.scooby.scooby.ui.DoctorProfileFragment"
            android:label="DoctorProfileFragment"
            tools:layout="@layout/fragment_doctor_profile"
            >
            <argument
                android:name="doctorId"
                app:argType="string" />
        </fragment>
        <fragment
            android:id="@+id/vetFragment"
            android:name="com.example.scooby.scooby.vet.VetFragment"
            android:label="fragment_vet"
            tools:layout="@layout/fragment_vet">
            <action
                android:id="@+id/action_vetFragment_to_doctorsFragment"
                app:destination="@id/doctorsFragment" />
            <action
                android:id="@+id/action_vetFragment_to_productFragment"
                app:destination="@id/productFragment" />
        </fragment>
        <fragment
            android:id="@+id/productFragment"
            android:name="com.example.scooby.scooby.product.fragment.ProductFragment"
            android:label="fragment_product"
            tools:layout="@layout/fragment_product">
            <action
                android:id="@+id/action_productFragment_to_productCartFragment"
                app:destination="@id/productCartFragment" />
        </fragment>
        <dialog
            android:id="@+id/menuBottomSheetFragment"
            android:name="com.example.scooby.scooby.ui.MenuBottomSheetFragment"
            android:label="menu_bottom_sheet"
            tools:layout="@layout/menu_bottom_sheet">
            <action
                android:id="@+id/action_menuBottomSheetFragment_to_vetFragment"
                app:destination="@id/vetFragment" />
            <action
                android:id="@+id/action_menuBottomSheetFragment_to_selectPetFragment"
                app:destination="@id/selectPetFragment" />
        </dialog>
        <fragment
            android:id="@+id/selectPetFragment"
            android:name="com.example.scooby.scooby.request.SelectPetFragment"
            android:label="fragment_select_pet"
            tools:layout="@layout/fragment_select_pet">
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <action
                android:id="@+id/action_selectPetFragment_to_dateRequestFragment"
                app:destination="@id/dateRequestFragment" />
            <action
                android:id="@+id/action_selectPetFragment_to_addPetsFragment"
                app:destination="@id/addPetsFragment" />
        </fragment>
        <fragment
            android:id="@+id/dateRequestFragment"
            android:name="com.example.scooby.scooby.request.DateRequestFragment"
            android:label="fragment_date_request"
            tools:layout="@layout/fragment_date_request">
            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <action
                android:id="@+id/action_dateRequestFragment_to_notesRequestFragment"
                app:destination="@id/notesRequestFragment" />
        </fragment>
        <fragment
            android:id="@+id/profileFragment"
            android:name="com.example.scooby.scooby.userProfile.ProfileFragment"
            android:label="fragment_profile"
            tools:layout="@layout/fragment_profile">
            <action
                android:id="@+id/action_profileFragment_to_editProfileFragment"
                app:destination="@id/editProfileFragment" />
            <action
                android:id="@+id/action_profileFragment_to_petProfileFragment"
                app:destination="@id/petProfileFragment" />
            <action
                android:id="@+id/action_profileFragment_to_myPetsFragment"
                app:destination="@id/myPetsFragment" />
            <action
                android:id="@+id/action_profileFragment_to_favoriteFragment"
                app:destination="@id/favoriteFragment" />
        </fragment>
        <fragment
            android:id="@+id/editProfileFragment"
            android:name="com.example.scooby.scooby.userProfile.EditProfileFragment"
            android:label="fragment_edit_profile"
            tools:layout="@layout/fragment_edit_profile" />
        <fragment
            android:id="@+id/petProfileFragment"
            android:name="com.example.scooby.scooby.userProfile.PetProfileFragment"
            android:label="fragment_pet_profile"
            tools:layout="@layout/fragment_pet_profile">
            <!--suppress NavigationFile -->
            <argument
                android:name="pet_position"
                app:argType="integer" />
        </fragment>
        <fragment
            android:id="@+id/myPetsFragment"
            android:name="com.example.scooby.scooby.addPet.MyPetsFragment"
            android:label="fragment_my_pets"
            tools:layout="@layout/fragment_my_pets">
            <action
                android:id="@+id/action_myPetsFragment_to_addPetsFragment"
                app:destination="@id/addPetsFragment"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        </fragment>
        <fragment
            android:id="@+id/addPetsFragment"
            android:name="com.example.scooby.scooby.addPet.NamePetFragment"
            android:label="fragment_add_pets"
            tools:layout="@layout/fragment_name_pet">
            <action
                android:id="@+id/action_addPetsFragment_to_sizePetFragment"
                app:destination="@id/sizePetFragment" />
        </fragment>
        <fragment
            android:id="@+id/sizePetFragment"
            android:name="com.example.scooby.scooby.addPet.SizePetFragment"
            android:label="fragment_size_pet"
            tools:layout="@layout/fragment_size_pet">
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_sizePetFragment_to_birthdayPetFragment"
                app:destination="@id/birthdayPetFragment" />

        </fragment>
        <fragment
            android:id="@+id/birthdayPetFragment"
            android:name="com.example.scooby.scooby.addPet.BirthdayPetFragment"
            android:label="fragment_birthday_pet"
            tools:layout="@layout/fragment_birthday_pet">
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_birthdayPetFragment_to_infoPetFragment"
                app:destination="@id/infoPetFragment" />
        </fragment>
        <fragment
            android:id="@+id/infoPetFragment"
            android:name="com.example.scooby.scooby.addPet.InfoPetFragment"
            android:label="fragment_info_pet"
            tools:layout="@layout/fragment_info_pet">
            <action
                android:id="@+id/action_infoPetFragment_to_submitPetFragment"
                app:destination="@id/submitPetFragment"
                app:popUpTo="@id/myPetsFragment"
                app:popUpToInclusive="true" />
            <argument
                android:name="listOfData"
                app:argType="string[]"
                app:nullable="true" />
        </fragment>
        <fragment
            android:id="@+id/submitPetFragment"
            android:name="com.example.scooby.scooby.addPet.SubmitPetFragment"
            android:label="fragment_submit_pet"
            tools:layout="@layout/fragment_submit_pet">
            <argument
                android:name="listOfData"
                app:argType="string[]"
                app:nullable="true" />
        </fragment>
        <fragment
            android:id="@+id/notesRequestFragment"
            android:name="com.example.scooby.scooby.request.NotesRequestFragment"
            android:label="fragment_notes_request"
            tools:layout="@layout/fragment_notes_request">

            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_notesRequestFragment_to_summaryRequestFragment"
                app:destination="@id/summaryRequestFragment" />
        </fragment>
        <fragment
            android:id="@+id/summaryRequestFragment"
            android:name="com.example.scooby.scooby.request.SummaryRequestFragment"
            android:label="fragment_summary_request"
            tools:layout="@layout/fragment_summary_request">

            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_summaryRequestFragment_to_confirmNumberFragment"
                app:destination="@id/confirmNumberFragment" />
        </fragment>
        <fragment
            android:id="@+id/paymentMethodsFragment"
            android:name="com.example.scooby.scooby.request.PaymentMethodsFragment"
            android:label="fragment_payment_methods"
            tools:layout="@layout/fragment_payment_methods">

            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <argument
                android:name="listOfData"
                app:argType="string[]" />

        </fragment>
        <fragment
            android:id="@+id/confirmServiceFragment"
            android:name="com.example.scooby.scooby.request.ConfirmServiceFragment"
            android:label="fragment_confirm_service"
            tools:layout="@layout/fragment_confirm_service">
            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_confirmServiceFragment_to_paymentMethodsFragment"
                app:destination="@id/paymentMethodsFragment" />
        </fragment>
        <fragment
            android:id="@+id/favoriteFragment"
            android:name="com.example.scooby.scooby.ui.FavoriteFragment"
            android:label="fragment_favorite"
            tools:layout="@layout/fragment_favorite" />
        <fragment
            android:id="@+id/confirmNumberFragment"
            android:name="com.example.scooby.scooby.request.ConfirmNumberFragment"
            android:label="fragment_confirm_number"
            tools:layout="@layout/fragment_confirm_number">
            <argument
                android:name="id_pets"
                app:argType="string[]" />
            <argument
                android:name="request_name"
                app:argType="string[]" />
            <argument
                android:name="listOfData"
                app:argType="string[]" />
            <action
                android:id="@+id/action_confirmNumberFragment_to_confirmServiceFragment"
                app:destination="@id/confirmServiceFragment" />
        </fragment>
        <fragment
            android:id="@+id/productCartFragment"
            android:name="com.example.scooby.scooby.product.fragment.ProductCartFragment"
            android:label="ProductCartFragment"
            tools:layout="@layout/fragment_product_cart"
            />
    </navigation>

    <navigation
        android:id="@+id/toPawsFragment"
        app:startDestination="@id/pawsFragment">

        <fragment
            android:id="@+id/pawsFragment"
            android:name="com.example.scooby.scooby.paws.fragment.PawsFragment"
            android:label="fragment_paws"
            tools:layout="@layout/fragment_paws">
            <action
                android:id="@+id/action_pawsFragment_to_shelterProfileFragment"
                app:destination="@id/shelterProfileFragment" />
        </fragment>
        <fragment
            android:id="@+id/shelterProfileFragment"
            android:name="com.example.scooby.scooby.paws.fragment.ShelterProfileFragment"
            android:label="ShelterProfileFragment"
            tools:layout="@layout/fragment_shelter_profile">
            <argument
                android:name="shelterId"
                app:argType="string" />
        </fragment>

    </navigation>

    <navigation
        android:id="@+id/toBookingFragment"
        app:startDestination="@id/bookingFragment">
        <fragment
            android:id="@+id/bookingFragment"
            android:name="com.example.scooby.scooby.ui.BookingFragment"
            android:label="fragment_booking"
            tools:layout="@layout/fragment_booking" />
    </navigation>

    <navigation
        android:id="@+id/toCommunityFragment"
        app:startDestination="@id/communityFragment">
        <fragment
            android:id="@+id/communityFragment"
            android:name="com.example.scooby.scooby.community.CommunityFragment"
            android:label="fragment_community"
            tools:layout="@layout/fragment_community" />
    </navigation>



</navigation>
