<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".scooby.AiResultFragment">
<androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="0dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/backBtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_back"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_matchRes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="53dp"
            android:fontFamily="@font/roboto_bold"
            android:text="@string/matching_results"
            android:textColor="@color/primary"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_yourUploadImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:fontFamily="@font/roboto_bold"
            android:text="@string/your_uploaded_photo"
            android:textColor="@color/_0f0f0f"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_matchRes" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/uploadedImg"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="@id/tv_yourUploadImage"
            app:layout_constraintStart_toStartOf="@id/tv_yourUploadImage"
            app:layout_constraintTop_toBottomOf="@id/tv_yourUploadImage" />

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cardLay_1"
            android:layout_width="match_parent"
            android:layout_height="309dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#F4F2F2"
            app:strokeWidth="0.5dp"
            app:strokeColor="#EEE2E2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/uploadedImg">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/roboto_medium"
                    android:text="@string/first_match_found"
                    android:textColor="@color/_0f0f0f"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/callBtn"
                    style="@style/Widget.Material3.Button.OutlinedButton"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="55dp"
                    android:layout_marginEnd="6dp"
                    android:fontFamily="@font/roboto_medium"
                    android:padding="0dp"
                    android:paddingStart="4dp"
                    android:text="@string/call"
                    android:textAlignment="center"
                    android:textColor="@color/black_font"
                    android:textSize="10sp"
                    app:cornerRadius="54dp"
                    app:icon="@drawable/call_icon"
                    app:iconGravity="textStart"
                    app:iconSize="8dp"
                    app:iconTint="@color/blue_icon"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:strokeColor="@color/black_30"
                    app:strokeWidth="0.5dp"
                    tools:ignore="SmallSp" />
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/foundPetImg1"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_marginStart="7dp"
                    android:layout_marginTop="55dp"
                    android:src="@drawable/placeholder"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_desc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/description"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/foundPetImg1"/>
                <TextView
                    android:id="@+id/petDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="7dp"
                    tools:text="Cute white cat will gold collar"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintStart_toEndOf="@id/tv_desc"
                    app:layout_constraintTop_toBottomOf="@id/foundPetImg1"/>
                <TextView
                    android:id="@+id/tv_current"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/current_owner"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_desc"/>
                <TextView
                    android:id="@+id/petCurrentOwn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="Ahmed Hany"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_current"
                    app:layout_constraintStart_toEndOf="@id/tv_current"
                    app:layout_constraintTop_toTopOf="@id/tv_current"/>
                <TextView
                    android:id="@+id/tv_phoneNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/phone_number"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_current"/>
                <TextView
                    android:id="@+id/petOwnerNm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="0102002020"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_phoneNum"
                    app:layout_constraintStart_toEndOf="@id/tv_phoneNum"
                    app:layout_constraintTop_toTopOf="@id/tv_phoneNum"/>
                <TextView
                    android:id="@+id/tv_location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/locationR"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_phoneNum"/>
                <TextView
                    android:id="@+id/petOwnerLoc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="Ismailia"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_location"
                    app:layout_constraintStart_toEndOf="@id/tv_location"
                    app:layout_constraintTop_toTopOf="@id/tv_location"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cardLay_2"
            android:layout_width="match_parent"
            android:layout_height="309dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#F4F2F2"
            app:strokeWidth="0.5dp"
            app:strokeColor="#EEE2E2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cardLay_1">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/roboto_medium"
                    android:text="@string/second_match_found"
                    android:textColor="@color/_0f0f0f"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
                <com.google.android.material.button.MaterialButton
                    android:id="@+id/callBtn2"
                    style="@style/Widget.Material3.Button.OutlinedButton"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="55dp"
                    android:layout_marginEnd="6dp"
                    android:fontFamily="@font/roboto_medium"
                    android:padding="0dp"
                    android:paddingStart="4dp"
                    android:text="@string/call"
                    android:textAlignment="center"
                    android:textColor="@color/black_font"
                    android:textSize="10sp"
                    app:cornerRadius="54dp"
                    app:icon="@drawable/call_icon"
                    app:iconGravity="textStart"
                    app:iconSize="8dp"
                    app:iconTint="@color/blue_icon"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:strokeColor="@color/black_30"
                    app:strokeWidth="0.5dp"
                    tools:ignore="SmallSp" />

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/foundPetImg2"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_marginStart="7dp"
                    android:layout_marginTop="55dp"
                    android:src="@drawable/placeholder"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_desc2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/description"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/foundPetImg2"/>
                <TextView
                    android:id="@+id/petDesc2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="7dp"
                    tools:text="Cute white cat will gold collar"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintStart_toEndOf="@id/tv_desc2"
                    app:layout_constraintTop_toBottomOf="@id/foundPetImg2"/>
                <TextView
                    android:id="@+id/tv_current2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/current_owner"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_desc2"/>
                <TextView
                    android:id="@+id/petCurrentOwn2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="Ahmed Hany"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_current2"
                    app:layout_constraintStart_toEndOf="@id/tv_current2"
                    app:layout_constraintTop_toTopOf="@id/tv_current2"/>
                <TextView
                    android:id="@+id/tv_phoneNum2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/phone_number"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_current2"/>
                <TextView
                    android:id="@+id/petOwnerNm2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="0102002020"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_phoneNum2"
                    app:layout_constraintStart_toEndOf="@id/tv_phoneNum2"
                    app:layout_constraintTop_toTopOf="@id/tv_phoneNum2"/>
                <TextView
                    android:id="@+id/tv_location2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginStart="7dp"
                    android:text="@string/locationR"
                    android:textSize="14sp"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/primary"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_phoneNum2"/>
                <TextView
                    android:id="@+id/petOwnerLoc2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="7dp"
                    tools:text="Ismailia"
                    android:textSize="12sp"
                    android:fontFamily="@font/roboto_medium"
                    android:textColor="@color/_0f0f0f"
                    app:layout_constraintBaseline_toBaselineOf="@id/tv_location2"
                    app:layout_constraintStart_toEndOf="@id/tv_location2"
                    app:layout_constraintTop_toTopOf="@id/tv_location2"/>
            </androidx.constraintlayout.widget.ConstraintLayout>


        </com.google.android.material.card.MaterialCardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>